{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react", "prettier"],
  "rules": {
    "semi": [2],
    "no-console": [1],
    "no-undef": [0],
    "no-unused-vars": [1],
    "no-multi-spaces": [1],
    "no-trailing-spaces": [1],
    "indent": [0],
    "prop-types": [0],

    "react/no-did-update-set-state": [2],
    "react/no-render-return-value": [0],
    "react/no-deprecated": [1]
  }
}
